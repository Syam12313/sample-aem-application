<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="userinfo"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[aemgeeks.components.author.editor]">
    <content
        granite:class="cmp-author_editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <well
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/well"
                        fieldLabel="Your Props">
                        <items jcr:primaryType="nt:unstructured">
                            <username
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter some date properly"
                                fieldLabel="UserName"
                                name="./username"/>
                            <password
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/password"
                                fieldDescription="Enter some date properly"
                                fieldLabel="Password"
                                name="./password"/>
                            <age
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                fieldDescription="Enter some date properly"
                                fieldLabel="Age"
                                name="./age"/>
                            <phonenumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="PhoneNumber">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./phonenumber">
                                    <items jcr:primaryType="nt:unstructured">
                                        <addphonenumber
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="HTML ID attribute to apply to the component."
                                            fieldLabel="AddPhoneNumber"
                                            name="./addphonenumber"/>
                                    </items>
                                </field>
                            </phonenumber>
                            <address
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter some date properly"
                                fieldLabel="Address"
                                name="./address"/>
                            <color
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                fieldDescription="Enter some date properly"
                                fieldLabel="Color"
                                name="./color"/>
                        </items>
                    </well>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
